/* PROJECT VIEW */
.project .gallery {
  padding-top: 115px;
}

.project .header {
  opacity: 0.9;
  height: 115px;
  position: fixed;
  background: white;
}
.project .menu {
  display: inline-block;
  background: white;
}



.project .menu .project-list .project-button-container a .text {
  display: none;
}

.project .menu .project-list .project-button-container.active a .text.title {
  display: inherit;
}
